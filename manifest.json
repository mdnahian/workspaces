{
  "name": "Workspaces",
  "description": "Save all your current tabs and come back to them later.",
  "version": "2",
  "manifest_version": 2,
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    "fonts/*"
  ],
  "permissions": [
    "storage",
    "tabs"
  ]
}
